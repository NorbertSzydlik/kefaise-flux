{
  "ecmaFeatures": {
    "blockBindings": true,
    "forOf": true,
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "semi": 2,
    "eqeqeq": [2, "allow-null"],
    "eol-last": 2,
    "no-extra-semi": 2,
    "indent": 2
  }
}
